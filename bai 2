#include "stdlib.h"
#include "stdio.h"

struct Finder {
    int num;
    struct Finder *Next;
};

typedef struct Finder *FindW;

int main() {
    int N, M, i;
    FindW t, a, b;

    printf("Please input N, M\n");
    scanf("%d %d", &N, &M);
    printf("Input was N=%d, M=%d\n", N, M);

    t = (FindW)malloc(sizeof(struct Finder));
    t->num = 1;

    a = t;
    for (i = 2; i <= n; i++) {
        b = (FindW)malloc(sizeof(struct Finder));
        b->num = i;
        a->Next = b;
        a = b;
    }
    a->Next = t;

    while (b->Next != b) {
        for (i = 1; i < m+1; i++)
            b = b->Next;
        a = b->Next;
        printf("%d, ", a->num);
        b->Next = a->Next;
        free(a);
    }
    printf("%d\n", b->num);
    free(b);
    return 0;
}
